[Unit]
Description=ulab access proxy daemon service
Documentation=https://github.com/Xuanfq/ulab-access-proxy
After=network.target ulab-access-proxy.service
Wants=ulab-access-proxy.service

[Service]
Type=simple
ExecStart=$NGINX_DAEMON_START_WITH_ARGS
ExecStop=$NGINX_DAEMON_STOP_WITH_ARGS
ExecQuit=$NGINX_DAEMON_QUIT_WITH_ARGS
TimeoutStartSec=120
PrivateTmp=true

[Install]
WantedBy=multi-user.target